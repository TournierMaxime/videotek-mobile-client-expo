{
  "expo": {
    "name": "VideoTek",
    "slug": "videotek-client-expo",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "bundleIdentifier": "com.hoggy.videotekclientexpo"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hoggy.videotekclientexpo",
      "versionCode": 29,
      "build": {
        "babel": {
          "include": ["@mod/mobile-common", "@mod/mobile-auth", "@mod/mobile-tmdb", "expo-modules-core"]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "f6110871-1ae7-43e7-8e85-5c2db50c2976"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f6110871-1ae7-43e7-8e85-5c2db50c2976"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notifications-icon.png",
          "mode": "production"
        }
      ],
      "expo-localization"
    ]
  }
}